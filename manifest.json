{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Search/filter tabs, close tabs/groups, view favicons, detect duplicates, highlight active tab. Created by Steve Souza using Claude Code (experimental)",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
